\tikzset{onestar/.pic={
\node[draw=black, circle, inner sep=0.75pt, line width=0.9mm] at (0,0)  {\normalsize\phantom{1}};
}}

\tikzset{twostar/.pic={
\node[text=black, rotate=45, inner sep=0pt] at (0,0) {\rule{6mm}{1mm}};
\node[text=black, rotate=-45, inner sep=0pt] at (0,0) {\rule{6mm}{1mm}};
}}

\tikzset{threestar/.pic={
\node[inner sep=0pt] at (0,0) {};
\node[regular polygon, regular polygon sides=3, draw==black, line width=0.9mm, inner sep=1.0pt] at (0,-0.7mm) {\normalsize\phantom{.}};
}}

\tikzset{fourstar/.pic={
\node[draw=black, fill=black, diamond, inner sep=2.125pt, line width=0.9mm] at (0,0) {\normalsize\phantom{.}};
}}

\tikzset{fivestar/.pic={
\node[inner sep=0pt] at (0,0) {};
\node[star, star points=5, star point ratio=2, fill=black, inner sep=1.6pt] at (0,-0.2mm) {\normalsize\phantom{.}};
}}

\tikzset{sixstar/.pic={
\node[regular polygon, regular polygon sides=6, fill=black, inner sep=3.2pt] at (0,0) {\normalsize\phantom{.}};
}}

\newcommand{\drawonestar}{\scalebox{0.75}{\tikz{\pic {onestar};}}}
\newcommand{\drawtwostar}{\scalebox{0.75}{\tikz{\pic {twostar};}}}
\newcommand{\drawthreestar}{\raisebox{0.0ex}{\scalebox{0.75}{\tikz{\pic {threestar};}}}}
\newcommand{\drawfourstar}{\scalebox{0.75}{\tikz{\pic {fourstar};}}}
\newcommand{\drawfivestar}{\scalebox{0.75}{\tikz{\pic {fivestar};}}}
\newcommand{\drawsixstar}{\raisebox{0.125ex}{\scalebox{0.75}{\tikz{\pic {sixstar};}}}}
